{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
    "MongoCollection": {
    "Database": "DayOfEating",
    "Collection": "does"
  },
    "ConnectionStrings": {
    "MongoDbConnection": "mongodb://host.docker.internal:27017"
  },
  "GrpcProducts":"https://localhost:6001",
  "GrpcMeals":"https://localhost:6021",
  "RabbitMQ": {
    "HostName": "host.docker.internal",
    "Port": "5672",
    "LogPublish": {
      "Exchange": "service.log.dx",
      "RoutingKey": "log.published.rk"
    },
    "ProductRemove": {
      "Exchange": "product.remove.fx",
      "QueueName": "product.remove.myday.q"
    }
  }
}
