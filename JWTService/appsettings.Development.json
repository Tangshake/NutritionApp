{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "ConnectionStrings": {
    "Secrets": "Server=host.docker.internal;Port=5433;Database=users;UserId=postgres;Password=postgres;"
  },
  "JwtToken": {
    "ExpiryTimeMinutes": "1"
  },
  "RefreshToken": {
    "Size": "128",
    "ExpiryTimeDays": "30"
  },
  "RabbitMQ": {
    "HostName": "localhost",
    "Port": "5672",
    "LogPublish": {
      "Exchange": "service.log.dx",
      "RoutingKey": "log.published.rk"
      }
  }
}
